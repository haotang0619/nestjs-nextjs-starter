{
  "name": "nestjs-nextjs-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "TAR=\"${TARGET:-serve}\"; nx run-many --target=$TAR --all",
    "start:prod": "pnpm start --prod",
    "start:story": "storybook dev -p 4400",
    "build": "TARGET=build pnpm start --verbose",
    "build:analy": "ANALYZE=true pnpm build --skip-nx-cache",
    "build:story": "storybook build",
    "lint": "TARGET=lint pnpm start --fix",
    "test": "TARGET=test pnpm start",
    "postinstall": "husky install"
  },
  "private": true,
  "lint-staged": {
    "./apps/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "10.3.9",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "10.3.9",
    "@nestjs/platform-express": "10.3.9",
    "@nestjs/swagger": "7.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.26.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.5.7",
    "safe-stable-stringify": "^2.4.3",
    "tslib": "^2.4.1",
    "ua-parser-js": "^1.0.35"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@nestjs/schematics": "10.1.2",
    "@nestjs/testing": "10.3.9",
    "@next/bundle-analyzer": "^13.3.1",
    "@nrwl/cli": "15.2.1",
    "@nrwl/cypress": "19.4.0",
    "@nrwl/eslint-plugin-nx": "19.4.0",
    "@nrwl/jest": "19.4.0",
    "@nrwl/linter": "19.4.0",
    "@nrwl/nest": "19.4.0",
    "@nrwl/next": "19.4.0",
    "@nrwl/node": "19.4.0",
    "@nrwl/react": "19.4.0",
    "@nrwl/web": "19.4.0",
    "@nrwl/workspace": "19.4.0",
    "@storybook/addon-essentials": "7.0.7",
    "@storybook/addon-interactions": "7.0.7",
    "@storybook/addon-links": "7.0.7",
    "@storybook/blocks": "7.0.7",
    "@storybook/react": "7.0.7",
    "@storybook/react-webpack5": "7.0.7",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/react": "15.0.6",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.1",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "babel-jest": "28.1.1",
    "cypress": "^11.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.2",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.0.3",
    "nx": "19.4.0",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "18.2.0",
    "storybook": "7.0.7",
    "storybook-code-panel": "0.2.1",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.4"
  }
}
